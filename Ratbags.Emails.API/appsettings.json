{
  "ConnectionStrings": {
    "DefaultConnection": "your database connection string"
  },
  "Ports": {
    "http": "your http port",
    "https": "your https port"
  },
  "Certificate": {
    "Name": "your certificate name",
    "Key": "your certificate key",
    "Path": "your certificate path",
    "Password": "your certificate password if using pfx"
  },
  "JWT": {
    "Secret": "yourSuperSecretKey3",
    "Issuer": "yourIssuer",
    "Audience": "yourAudience"
  },
  "Messaging": {
    "Hostname": "your rabbitmq hostname",
    "VirtualHost": "your rabbitmq virtual hostname",
    "Username": "your rabbitmq username",
    "Password": "your rabbitmq password"
  },
  "Mail": {
    "NoReplyEmail": "your no reply email address",
    "SmtpSettings": {
      "Host": "your host",
      "Port": "your port number",
      "EnableSsl": false,
      "Username": "your username ",
      "Password": "your password"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
